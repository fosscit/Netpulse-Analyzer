{
  "manifest_version": 2,
  "name": "Netpulse Analyzer",
  "version": "1.0",
  "description": "Monitor network speed in Chrome.",
  "permissions": ["activeTab", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
